{
    "name": "yia-web",
    "version": "0.0.1",
    "scripts": {
        "dev": "svelte-kit dev -p 3131",
        "build": "svelte-kit build",
        "package": "svelte-kit package",
        "preview": "svelte-kit preview",
        "generate": "run-s generateTask:*",
        "generateTask:generate": "graphql-codegen --config codegen.yml",
        "generateTask:cleanup": "scripts/replaceExports.sh",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.3.1",
        "@graphql-codegen/typescript": "2.4.2",
        "@graphql-codegen/typescript-operations": "^2.2.2",
        "@graphql-codegen/typescript-urql": "^3.4.2",
        "@graphql-codegen/urql-svelte-operations-store": "^1.1.9",
        "@sveltejs/adapter-auto": "next",
        "@sveltejs/adapter-node": "^1.0.0-next.68",
        "@sveltejs/adapter-static": "^1.0.0-next.28",
        "@sveltejs/kit": "next",
        "@types/lodash": "^4.14.178",
        "@typescript-eslint/parser": "^5.9.1",
        "@urql/exchange-graphcache": "^4.3.6",
        "@urql/svelte": "^1.3.3",
        "eslint-plugin-svelte3": "^3.4.0",
        "graphql": "^16.3.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "sass": "^1.49.7",
        "svelte": "^3.46.4",
        "svelte-check": "^2.2.6",
        "svelte-loader": "^3.1.2",
        "svelte-preprocess": "^4.10.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3",
        "urql": "^2.0.6"
    },
    "type": "module",
    "volta": {
        "node": "17.3.1"
    },
    "dependencies": {
        "lodash": "^4.17.21",
        "lodash-es": "^4.17.21"
    }
}
